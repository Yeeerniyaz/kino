{
	"expo": {
		"name": "kino",
		"slug": "kino",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.kino.kino",
			"buildNumber": "1.0.0",
			"infoPlist": {
				"NSCameraUsageDescription": "This app uses the camera to scan barcodes.",
				"NSPhotoLibraryUsageDescription": "This app uses the photo library to scan barcodes."
			},
			"userInterfaceStyle": "automatic",
		},
		"android": {
			"package": "com.kino.kino",
			"versionCode": 1,
			"userInterfaceStyle": "automatic",
			"permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
